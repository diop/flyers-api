{
  "name": "flyers-api",
  "version": "1.0.0",
  "description": "Flyers.ai Backend Api",
  "main": "src/server.js",
  "scripts": {
    "start": "NODE_ENV='production' node src/server.js ",
    "start:dev": "nodemon src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:create": "createdb flyers",
    "db:drop": "dropdb flyers",
    "db:schema": "psql -d flyers < src/database/schema.sql",
    "db:seed": "psql flyers < ./src/database/load-data.sql",
    "db:init": "npm run db:drop; npm run db:create; npm run db:schema; npm run db:seed"
  },
  "keywords": [
    "Flyers.ai"
  ],
  "author": "Patrick Porché, Jason Emberley, Zubair Ahmed, Fodé Diop",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "node-mailer": "^0.1.1",
    "nodemailer": "^4.4.1",
    "pg-promise": "^7.3.3",
    "qr-image": "^3.2.0",
    "zeppelin-solidity": "^1.5.0"
  },
  "devDependencies": {
    "nodemon": "^1.14.11"
  }
}
